  <ItemGroup>
    <PackageReference Include="Swashbuckle.AspNetCore" Version="6.6.2" />
    <PackageReference Include="MassTransit.RabbitMQ" Version="8.2.5" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="Relatorios\" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="Relatorios\" />
  </ItemGroup>
</Project>
